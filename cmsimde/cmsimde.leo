<t tx="leo.20190515144238.23">def sizeof_fmt(num):

    """Size formate
    """

    for x in ['bytes','KB','MB','GB']:
        if num &lt; 1024.0:
            return "%3.1f%s" % (num, x)
        num /= 1024.0
    return "%3.1f%s" % (num, 'TB')


