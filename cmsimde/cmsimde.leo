    # Define the set of special characters to remove
    special_chars = ['!', '@', '#', '$', '%', '^', '&amp;', '*', '(', ')', '+', '=', '[', ']', '{', '}', '|', '\\', '/',
                     ':', ';', '&lt;', '&gt;', ',', '.', '?', '`', '~', "'", '"']
